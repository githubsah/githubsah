list=[]
min=int(input("enter the low limit:"))
max=int(input("enter the max limit:"))
for n in range (min,max+1):
    if n > 1 :
        for i in range(2,n):
            if n%i == 0 :
              
              break
        else : 
            print(n)
            list.append(n)
print(list)
print(f"the length of the list is: {len(list)}")
